{
	"name": "courier-notices",
	"version": "1.5.7",
	"description": "A way to display, manage, and control front end notifications for users, globally or individually, in the header,footer, by shortcode or by page.",
	"main": "gulpfile.babel.js",
	"scripts": {
		"start": "gulp",
		"build": "gulp build",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commit-msg"
		}
	},
	"author": "Linchpin <sayhi@linchpin.com>",
	"license": "GPL-2.0-or-later",
	"dependencies": {
		"jquery": "3.6.3",
		"lodash": "4.17.21",
		"save-dev": "0.0.1-security",
		"what-input": "5.2.12"
	},
	"devDependencies": {
		"@babel/core": "7.18.13",
		"@babel/preset-env": "7.18.10",
		"@babel/register": "7.18.9",
		"autoprefixer": "10.4.8",
		"babel-loader": "9.1.2",
		"browser-sync": "2.27.10",
		"fs-extra": "^11.1.0",
		"gulp": "4.0.2",
		"gulp-babel": "8.0.0",
		"gulp-cli": "2.3.0",
		"gulp-concat": "2.6.1",
		"gulp-if": "3.0.0",
		"gulp-sass": "5.1.0",
		"gulp-postcss": "9.0.1",
		"postcss": "^8.4.18",
		"gulp-sourcemaps": "3.0.0",
		"gulp-uglify": "3.0.2",
		"husky": "^8.0.1",
		"sass": "^1.54.8",
		"webpack": "5.73.0",
		"webpack-stream": "7.0.0",
		"yargs": "17.5.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/linchpin/courier-notices.git"
	},
	"bugs": {
		"url": "https://github.com/linchpin/courier-notices/issues",
		"email": "sayhi@linchpin.com"
	},
	"engines": {
		"node": ">= v16.19.1"
	},
	"browserslist": [
		"defaults"
	],
	"output": {
		"chunkLoading": false,
		"wasmLoading": false
	},
	"resolutions": {
		"glob-parent": "6.0.2",
		"semver": "7.3.8",
		"trim-newlines": "4.0.2",
		"meow": "10.1.5",
		"chokidar": "3.5.3"
	},
	"buildconfig": {
		"webpack": {
			"entry": {
				"courier-notices": "./assets/js/courier-notices.js",
				"courier-notices-admin": "./assets/js/courier-notices-admin.js"
			},
			"output": {
				"filename": "[name].js"
			},
			"module": {
				"rules": [
					{
						"test": "/\\.js$/",
						"use": {
							"loader": "babel-loader",
							"options": {
								"presets": [
									"@babel/preset-env"
								],
								"compact": false
							}
						}
					}
				]
			},
			"externals": {
				"jquery": "jQuery"
			}
		},
		"gulp": {
			"fonts": {
				"assets": [
					"./assets/fonts/**/*"
				],
				"dest": "./css/fonts"
			},
			"sass": {
				"assets": [
					"assets/scss/**/*.scss"
				],
				"compatibility": ">ie11",
				"dest": "./css"
			},
			"javascript": {
				"assets": "assets/js/**/*.js",
				"libraries": [],
				"dest": "./js"
			}
		}
	}
}
